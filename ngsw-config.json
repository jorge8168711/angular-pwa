{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/css?family=Noto+Sans:400,700",
          "http://i.annihil.us/u/prod/marvel/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "characters",
      "urls": [
        "https://gateway.marvel.com:443/v1/public/characters",
        "http://i.annihil.us/u/prod/marvel/**"
      ],
      "cacheConfig": {
        "maxSize": "2000",
        "maxAge": "2d",
        "timeOut": "10s",
        "strategy": "freshness"
      }
    }
  ]
}
